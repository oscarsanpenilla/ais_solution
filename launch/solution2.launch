<launch>
    <param name ="/use_sim_time" value="true"/>
    <node pkg="rosbag" type="play" name="player" output="screen" args="-l --clock ../bagfiles/pot-and-static-objects.bag"/>
    <node pkg="ais_solution" type="solution2" name="solution2" output="screen" />
    <node pkg="ais_solution" type="cloud_transform" name="cloud_transformer" output="screen" />

    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find ais_solution)/launch/config.rviz" />
</launch>