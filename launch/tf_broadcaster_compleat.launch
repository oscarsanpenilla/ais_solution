<launch>
    <param name ="/use_sim_time" value="true"/>
    <node pkg="rosbag" type="play" name="player"  args="-l --clock ../bagfiles/pot-and-static-objects.bag"/>
    <!-- <node pkg="tf" type="static_transform_publisher" name="world_pub" args="0 0 0 0 0 0 map world 100"/> -->
    <node pkg="ais_solution" type="tf_broadcaster_compleat" name="tf_broadcaster_compleat" output="screen" />
    <node pkg="ais_solution" type="cloud_transform" name="cloud_transformer" output="screen" />
    <node pkg="ais_solution" type="laser_transformer" name="laser_transformer" output="screen" /> 
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find ais_solution)/launch/config.rviz" />
</launch>